{
  "name": "liv",
  "version": "1.0.0",
  "description": "test-installer",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node app.js",
    "ignore: it is a comment: --ext will instruct nodemon to only look for the changes in specified files.": "",
    "dev": "nodemon --ext js,html,css app.js",
    "build": "npx pkg -t win,linux,macos -p node_modules,public,routes,utils,javaProgram . --out-path release"
  },
  "bin": {
    "liv": "app.js"
  },
  "pkg": {
    "scripts": [
      "routes/*.js",
      "utils/*.js"
    ],
    "assets": [
      "public/css/*",
      "public/js/*",
      "public/*.html"
    ]
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "multer": "^1.4.5-lts.1",
    "pkg": "^5.8.1",
    "uuid": "^9.0.0",
    "window": "^4.2.7"
  },
  "devDependencies": {
    "nodemon": "^2.0.21"
  },
  "author": "Ishu,Lave,Vanshika",
  "license": "ISC"
}
